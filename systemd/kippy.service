
[Unit]

Description=Kippy

StartLimitIntervalSec=5min
StartLimitBurst=5

[Service]

ExecStart=/usr/bin/python3.8 /home/bots/launcher.py kippy

Environment=PYTHONUNBUFFERED=1

Restart=on-failure

TimeoutStopSec=10s

Type=notify

User=bots

WatchdogSec=30s

KillSignal=SIGINT
WatchdogSignal=SIGINT

NotifyAccess=all


[Install]

WantedBy=default.target